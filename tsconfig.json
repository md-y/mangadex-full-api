{
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "Node",
        "module": "CommonJS",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "lib": ["es2015"],
    "include": ["./src/**/*"]
}
