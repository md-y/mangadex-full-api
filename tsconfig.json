{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node16",
    "module": "CommonJS",
    "outDir": "./dist/cjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": false,
    "removeComments": true
  },
  "lib": [
    "es2015",
  ],
  "include": [
    "./src/**/*"
  ],
}
