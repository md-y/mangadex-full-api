{
  "name": "mangadex-full-api",
  "version": "6.0.0",
  "description": "A MangaDex api based around the official API.",
  "types": "./dist/types/index.d.mts",
  "scripts": {
    "lint": "eslint . --ext .ts,.mjs,.cjs,.js",
    "build:schema": "node ./scripts/buildSchema.mjs",
    "build:tsc": "tsc && tsc --outDir ./dist/esm --module ES2020 && tsc --declaration true --emitDeclarationOnly true --declarationDir ./dist/types --removeComments false",
    "prepublish": "npm run build:schema & npm run build:tsc"
  },
  "engines": {
    "node": ">=19.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/md-y/mangadex-full-api.git"
  },
  "keywords": [
    "mangadex",
    "api",
    "manga",
    "anime",
    "manwha",
    "manhua"
  ],
  "author": "m;dy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/md-y/mangadex-full-api/issues"
  },
  "homepage": "https://md-y.github.io/mangadex-full-api/",
  "devDependencies": {
    "@microsoft/tsdoc": "^0.14.2",
    "@microsoft/tsdoc-config": "^0.16.2",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "dotenv": "^14.3.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "prettier": "^2.8.1",
    "swagger-typescript-api": "^12.0.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "oauth4webapi": "^2.0.6"
  },
  "exports": {
    ".": {
      "default": {
        "default": "./dist/esm/index.mjs",
        "import": "./dist/esm/index.mjs",
        "require": "./dist/cjs/index.cjs",
        "types": "./dist/types/node.d.mts"
      }
    }
  }
}
